<div class="form-group row">
  <label class="col-form-label col-sm-2" for="question">Question</label>
  <div class="col-sm-8">
    <input autofocus class="form-control" name="question" value="<%= model.question %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2" for="reponse">Answer</label>
  <div class="col-sm-7">
    <input class="form-control" name="reponse" value="<%= model.reponse %>" />
  </div>
</div>
